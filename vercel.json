{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "cloud_cut/src/app/api/convert/route.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "dest": "cloud_cut/src/app/api/convert/route.py"
    }
  ],
  "functions": {
    "src/app/api/inserts/add/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "PYTHONPATH": "/usr/bin/python3"
  }
} 