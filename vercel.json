{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "src/app/api/convert/route.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "dest": "src/app/api/convert/route.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "functions": {
    "src/app/api/inserts/add/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "PYTHONPATH": "/usr/bin/python3"
  }
} 